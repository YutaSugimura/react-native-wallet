{
  "name": "template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}' --quiet",
    "lint:fix": "eslint '**/*.{js,jsx,ts,tsx}' --quiet --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "bundle-visualize": "./node_modules/.bin/react-native-bundle-visualizer",
    "ios:clean": "cd ios && rm -fr Pods Podfile.lock && pod install && cd ..",
    "ios:pod": "cd ios && pod install && cd ..",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build": "cd android && ./gradlew bundleRelease && cd ..",
    "android:releaseTest": "react-native run-android --variant=release",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -o ./build/site"
  },
  "dependencies": {
    "events": "^1.1.1",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-crypto": "^2.2.0",
    "react-native-randombytes": "^3.6.1",
    "readable-stream": "^1.0.33",
    "stream-browserify": "^1.0.0",
    "url": "^0.10.3",
    "util": "^0.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/react-hooks": "^5.1.0",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@welldone-software/why-did-you-render": "^6.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "react-dom": "^17.0.2",
    "react-native-bundle-visualizer": "^2.2.1",
    "react-native-web": "^0.15.0",
    "react-test-renderer": "17.0.2",
    "rn-nodeify": "^10.3.0",
    "typescript": "4.2.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "./jest/jest.setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "yarn check-types"
    ]
  },
  "react-native": {
    "console": "console-browserify",
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "console": "console-browserify",
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
